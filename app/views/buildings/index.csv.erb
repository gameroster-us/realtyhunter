<%- headers = ['Street Address', 'Neighborhood', 'Borough',
	'City', 'State', 'Zipcode', 'Active Units',
	'Total Units', 'Last Updated At', 'Notes',
	'Landlord', 'Amenities', 'Utilities', 'Rental Term',
  'Total Units Count',
  'Active Units Count',
  'Last Unit Updated At',
  'Created At', 'Updated At'
  ] -%>
<%= CSV.generate_line headers %>
<%- @buildings.each do |building| -%>

  <%= [building.street_address, building.neighborhood_name, building.sublocality,
  	building.administrative_area_level_2_short,
  		building.administrative_area_level_1_short, building.postal_code, building.active_units.count,
  		building.units.count, building.updated_at, building.notes,
  		building.landlord.code, building.amenities_to_s, building.utilities_to_s,

      building.rental_term ? building.rental_term.name.titleize : '',

      building.total_unit_count,
      building.active_unit_count,
      building.last_unit_updated_at,

      building.created_at,
      building.updated_at

      ].to_csv.html_safe %>
<%- end -%>