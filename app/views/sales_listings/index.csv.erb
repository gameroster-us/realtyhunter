<%- headers = [
  'Full Address', 'Unit', 'Neighborhood', 'Is Exclusive',
  'Beds', 'Baths', 'Tenant Occupied', 'Internal Notes', "Public Desription",
  'Listing Type', 'Percent Commission', 'Outside Broker Commission', 'Seller Name',
  'Seller Phone', 'Seller Address', 'Year Built', 'Building Type', 'Lot Size',
  'Building Size', 'Block Taxes', 'Lot Taxes', 'Water Sewer', 'Insurance', 'School District',
  'Certificate of Occupancy', 'Violation Search',
  'Primary Agent', 'Listing ID', 'Price', 'Available', 'Access Info', 
  'Status', 'Created At', 'Updated At', 'Archived',
	] -%>

<%= CSV.generate_line headers %>
<%- @sales_units.each do |listing| -%>
	
  <%= [listing.formatted_street_address, listing.building_unit, listing.neighborhood_name, listing.exclusive,
      listing.beds, listing.baths, listing.tenant_occupied, listing.internal_notes, listing.public_description,
      listing.listing_type, listing.percent_commission, listing.outside_broker_commission, listing.seller_name,
      listing.seller_phone, listing.seller_address, listing.year_built, listing.building_type, listing.lot_size,
      listing.building_size, listing.block_taxes, listing.lot_taxes, listing.water_sewer, listing.insurance,
      listing.school_district, listing.certificate_of_occupancy, listing.violation_search, 
      listing.primary_agent_id ? @agents[listing.primary_agent_id][0].name : '', 
      listing.listing_id, listing.rent, 
      listing.available_by, listing.access_info, listing.status, listing.created_at, listing.updated_at, listing.archived
    ].to_csv.html_safe %>
<%- end -%>