<% if @uer.new_record? %>
	$('input#user_name').effect('highlight', {color: 'red'});
<% else %>
	$('div#users').append("<%= escape_javascript(render @user) %>")
	$('div#<%= dom_id(@user) %>').effect('highlight')
<% end %>