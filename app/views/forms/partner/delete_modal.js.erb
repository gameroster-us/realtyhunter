$('#entryDeleteModal .delete-btn').replaceWith("<%= j(link_to 'Mark Inactive', forms_partner_path(@entry), remote: true, method: :delete, class: 'btn btn-danger link-as-button', id: 'delete-link') %>");
$('#entryDeleteModal #delete-link').click( function(event) {
	$('#entryDeleteModal').modal('hide');
	event.preventDefault();
});

$('#entryDeleteModal').modal('show');
