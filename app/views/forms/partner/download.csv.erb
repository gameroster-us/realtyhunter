<%- headers = [
	'Name', 
	'Phone Number', 
	'Email',
	'How Did You Hear About Us?', 
	'Street Address',
  'Address Line 2',
	'City',
  'State_Province_Region',
  'Postal Zip Code',
  'Country',
  'Number of Bedrooms',
  'Renovated',
  'Heat Included',
  'Hot Water Included',
  'Gas Included',
  'Electric Included',
  'No Utilities Included',
  'Move In Date',
  'Archived',
	'Created At', 'Updated at'] -%>
<%= CSV.generate_line headers %>
<%- @entries.each do |entry| -%>
  <%= [
  	entry.name, 
  	entry.phone_number, 
  	entry.email,
  	entry.address_street_address, 
    entry.address_address_line_2,
    entry.address_city,
    entry.address_state_province_region,
    entry.address_postal_zip_code,
    entry.address_country,
    entry.number_of_bedrooms,
    entry.renovated,
    entry.utilities_heat_included,
    entry.utilities_hot_water_included,
    entry.utilities_gas_included,
    entry.utilities_electric_included,
    entry.utilities_no_utilities_included,
    entry.move_in_date,
  	entry.archived,
		entry.created_at, 
  	entry.updated_at
  	].to_csv.html_safe %>
<%- end -%>
