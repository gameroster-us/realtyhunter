  <tr class="<%= cycle('even', 'odd') -%>" data-id="<%= entry.id %>" data-email="<%= entry.email %>" >
    
    <!-- ****** checkboxes ****************************************** -->
    <td>
      <i data-action="selectUnit" class="fa fa-square-o"></i>
    </td>

    <!-- ****** NAME ****************************************** -->
    <td>
      <%= link_to entry.name, detail_modal_forms_residential_listing_path(entry), 
        data: { toggle: "modal", target: "#entryDetailModal"}, remote: true %>
    </td>

    <!-- ****** PHONE ****************************************** -->
    <td><%= entry.phone_number %></td>

    <!-- ****** EMAIL ****************************************** -->
    <td><%= entry.email %></td>

    <td><%= entry.message %></td>

    <td><%= entry.created_at %></td>

    <!-- ****** LINKS ****************************************** -->
    <% if current_user.is_management? || current_user.is_listings_manager? %>
      <td>
        <% if !entry.archived %>
          <%= link_to 'Mark Inactive', delete_modal_forms_residential_listing_path(entry), 
            data: { toggle: "modal", target: "#entryDeleteModal"}, remote: true %>
        <% else %>
          <%= link_to 'Mark Active', unarchive_modal_forms_residential_listing_path(entry), 
            data: { toggle: "modal", target: "#entryUnarchiveModal"}, remote: true %>
        <% end %>
        <br />
      </td>
    <% end %>
  </tr>
