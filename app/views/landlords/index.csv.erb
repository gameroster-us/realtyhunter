<%- headers = ['Name', 'Code', 'Mobile', 'Phone', 'Fax', 'Email', 'Website', 'Street Address', 'City', 'State', 'Zipcode',
	'Notes', 'Management Info', 'Listing Agent Percentage', 'Pet Policy', 'Required Security', 'Created At', 'Updated at'] -%>
<%= CSV.generate_line headers %>
<%- @landlords.each do |landlord| -%>
  <%= CSV.generate_line([landlord.name, landlord.code, landlord.mobile, landlord.phone, landlord.fax, landlord.email, landlord.website, landlord.street_address, landlord.city, landlord.state, landlord.zipcode, landlord.notes, 
  	landlord.management_info, landlord.listing_agent_percentage, landlord.pet_policy.name, 
  	landlord.required_security.name, landlord.created_at.strftime("%Y-%b-%d"), landlord.updated_at.strftime("%Y-%b-%d")]) %>
<%- end -%>
