<%- headers = [
	'Name',
	'Code',
	'Contact Name',
	'Office Phone',
	'Mobile',
	'Fax',
	'Email',
	'Website',
	'Street Address',
	'Notes',
	'Management Info',
	'Key Pick Up Location',
	'Update Source',
	'Listing Agent Percentage',
	'Has Fee',
	'OP Fee %',
	'TP Fee %',
  'Total Units Count',
  'Active Units Count',
  'Last Unit Updated At',
	'Created At', 'Updated at'] -%>
<%= CSV.generate_line headers %>
<%- @landlords.each do |landlord| -%>
  <%= [
  	landlord.name,
  	landlord.code,
  	landlord.contact_name,
  	landlord.office_phone,
  	landlord.mobile,
  	landlord.fax,
  	landlord.email,
  	landlord.website,
  	landlord.formatted_street_address,
  	landlord.notes,
  	landlord.management_info,
  	landlord.key_pick_up_location,
  	landlord.update_source,
  	landlord.listing_agent_percentage,

		landlord.has_fee ? 'Yes' : 'No',
  	landlord.op_fee_percentage,
  	landlord.tp_fee_percentage,

    landlord.total_unit_count,
    landlord.active_unit_count,
    landlord.last_unit_updated_at,

  	landlord.created_at,
  	landlord.updated_at
  	].to_csv.html_safe %>
<%- end -%>
