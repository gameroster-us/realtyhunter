<tr class="<%= cycle('even', 'odd') -%>">
  <td><%= link_to landlord.code, landlord %></td>
  <td><%= landlord.name %></td>
  <td class="js-phoneNumber"><%= landlord.mobile %></td>
  <td>
    <% if landlord.listing_agent_name %>
      <%= link_to landlord.listing_agent_name, user_path(landlord.listing_agent_id) %>
    <% else %>
      --
    <% end %>
  </td>
  <td>
    <% if landlord.listing_agent_percentage %>
      <%= landlord.listing_agent_percentage %>
    <% else %>
      --
    <% end %>
  </td>
  <td><%= landlord.active_unit_count %></td>
  <td><%= landlord.total_unit_count %></td>
  <td>
    <% if landlord.last_unit_updated_at %>
      <%= time_ago_in_words(landlord.updated_at) %>
    <% else %>
      --
    <% end %>
  </td>
  <td><%= landlord.updated_at %></td>
  <td class="hidden-print">
    <% if current_user.is_management? %>
      <%= link_to 'Edit', edit_landlord_path(landlord) %>
      <br>
      <%= link_to 'Delete', delete_modal_landlord_path(landlord),
      data: {toggle: "modal", target: "#landlordDeleteModal", params: params}, remote: true %>
    <% end %>
  </td>
</tr>
